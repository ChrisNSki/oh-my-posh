{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#232f36",
          "foreground": "#bb86fc",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": "  \uf489 {{ .Name }}  ",
          "trailing_diamond": "\ue0b0",
          "type": "shell"
        },
        {
          "background": "#232f36",
          "foreground": "#bb86fc",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": true,
            "fetch_version": true,
            "yarn_icon": "\ue6a7",
            "npm_icon": "\ue718"
          },
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
        {
          "type": "npm",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "background": "#232f36",
          "foreground": "#bb86fc",
          "template": "\ue5fa {{ .Full }} "
        },
        {
          "type": "sysinfo",
          "background": "#232f36",
          "foreground": "#bb86fc",
          "leading_diamond": "\ue0b2",
          "style": "diamond",
          "template": " \uf4bc  MEM: {{ round .PhysicalPercentUsed .Precision }}% | {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }} GB ",
          "properties": {
            "precision": 4
          },
          "trailing_diamond": "\ue0b0"
        },
        {
          "background": "#232f36",
          "foreground": "#bb86fc",
          "leading_diamond": "\ue0b2",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " Execution Time: {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#232f36",
          "foreground": "#bb86fc",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#bb86fc",
          "foreground": "#232f36",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFFFFF{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFFFFF{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }} <,#FFFFFF>{{ .HEAD }}</>{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          }
        },
        {
          "background": "#bb86fc",
          "foreground": "#232f36",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }} {{ .Venv }} {{ end }} {{ .Full }} {{ end }} ",
          "type": "python"
        },
        {
          "background": "#bb86fc",
          "foreground": "#232f36",
          "powerline_symbol": "\ue0b0 ",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
